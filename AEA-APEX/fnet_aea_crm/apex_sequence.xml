<?xml version="1.0" encoding="utf-8"?>
<openerp>
  <data> 
      
      <!-- Lorry Receipt-->
     <record id="seq_type_lorry_receipt" model="ir.sequence.type">
            <field name="name">Lorry Receipt</field>
            <field name="code">lorry.receipt</field>
        </record>

        <record id="seq_lorry_receipt" model="ir.sequence">
            <field name="name">Lorry Receipt</field>
