<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data noupdate="1">

        <!-- Sequences for CRM Opportunity -->
        <record id="seq_type_tour_approval" model="ir.sequence.type">
            <field name="name">Oppotunity</field>
            <field name="code">crm.lead</field>
        </record>

        <record id="crm_oppor_inherit_seq" model="ir.sequence">
            <field name="name">Opportunity</field>
            <field name="code">crm.lead</field>
            <field name="prefix">ENQ</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>
        
        
         <!-- Sequences for sale.quotation -->
        <record id="seq_type_sale_quotations" model="ir.sequence.type">
            <field name="name">Sales Quotation</field>
            <field name="code">sale.quotations</field>
        </record>

        <record id="seq_sale_quotations" model="ir.sequence">
            <field name="name">Sales Quotation</field>
            <field name="code">sale.quotations</field>
            <field name="prefix">SQ</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>



    </data>
</openerp>
